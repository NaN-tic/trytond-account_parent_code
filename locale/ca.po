msgctxt "model:ir.message,text:account_from_template"
msgid ""
"You can not modify/delete account \"%(account)s\" because it is created from"
" an account template"
msgstr ""
"No podeu modificar/esborrar el compte \"%(account)s\" perquè es crea a "
"partir d’una plantilla de compte"

msgctxt "model:ir.message,text:msg_account_code_unique"
msgid "Account code must be unique."
msgstr "El codi de compte ha de ser únic."

msgctxt "model:ir.message,text:msg_account_code_unique_per_company"
msgid "Account code must be unique per company."
msgstr "El codi de compte ha de ser únic per a cada empresa."
